      Paths_autodocodec_api_usage
  hs-source-dirs:
      test/
  ghc-options: -threaded -rtsopts -with-rtsopts=-N -Wall
  build-tool-depends:
      sydtest-discover:sydtest-discover
  build-depends:
      QuickCheck
    , aeson
    , autodocodec
    , autodocodec-api-usage
    , autodocodec-openapi3
    , autodocodec-schema
    , autodocodec-swagger2
    , autodocodec-yaml
